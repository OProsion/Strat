#include <iostream>
using namespace std;
int main()
{
	long long world_pp, us_pp;
	cout << "Enter the world's population: ";
	cin >> world_pp;
	cout << "Enter the population of US: ";
	cin >> us_pp;
	cout << "The population of the US is "<<100*double(us_pp)/double(world_pp)<<"% of the world population.\n";
	return 0;
}
